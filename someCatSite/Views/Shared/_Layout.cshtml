<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>@ViewData["Title"] - Cat Site</title>
    <link rel="stylesheet" href="~/css/baseLayout.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/css/someCatSite.styles.css"/>
</head>
<body>
<header>
    <nav>
        <ul>
            <li>
                <a asp-controller="Home" asp-action="Index">Home</a>
            </li>
            <li>
                <a asp-controller="Home" asp-action="CatYears">Cat Years to Human Years</a>
            </li>
            <li>
                <a asp-controller="Home" asp-action="CatGallery">Cat Gallery</a>
            </li>
            <li>
                <a asp-controller="Home" asp-action="CalorieNeeds">Calculate Calorie Needs</a>
            </li>
        </ul>
    </nav>
</header>
@RenderBody()
<footer>
    <div>
        &copy; 2024 - someCatSite - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
    </div>
</footer>
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="~/js/site.js" asp-append-version="true"></script>
@await RenderSectionAsync("Scripts", required: false)
</body>
</html>